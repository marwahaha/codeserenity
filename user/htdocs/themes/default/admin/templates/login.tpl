<form action="" method="post" class="cs_form">
	<fieldset class="cs_fieldset">
		<div class="cs_field">
			<div class="cs_label"><label for="login_username">Username</label></div>
			<div class="cs_input"><input type="text" name="username" id="login_username" class="cs_form_text" /></div>
		</div>
		<div class="cs_field">
			<div class="cs_label"><label for="login_password">Password</label></div>
			<div class="cs_input"><input type="password" name="password" id="login_password" class="cs_form_text" /></div>
		</div>
		<div class="cs_button">
			<input type="submit" name="login" class="cs_form_submit" value="Log in" />
		</div>
	</fieldset>
</form>